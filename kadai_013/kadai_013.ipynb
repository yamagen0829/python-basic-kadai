      "source": [
        "def calculate_total(price: int, consumption_tax_rate: int):\n",
        "  consumption_tax = price * (consumption_tax_rate / 100)\n",
        "  total = price + consumption_tax\n",
        "\n",
        "  return total\n",
        "\n",
        "total_price = calculate_total(110, 10)\n",
        "\n",
        "print(f\"{total_price}円\")\n"
      ]
