        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    #コンストラクタを定義する\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(self.name)\n",
        "    print(self.age)\n",
        "\n",
        "#インスタンス化する\n",
        "human = Human(\"侍太郎\", 36)\n",
        "\n",
        "#メソッドにアクセスして実行する\n",
        "human.printinfo()"
